{
  "name": "docs",
  "private": true,
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "test:docs": "npx runme run --all --category examples --skip-prompts",
    "test:examples": "ts-node-test examples/",
    "build:docs": "find . -name \"*.md\" -type f -maxdepth 1 ! -name \"README.md\" -delete && python build_docs.py"
  },
  "dependencies": {
    "@vechain-sdk/core": "*"
  },
  "devDependencies": {
    "expect": "^29.7.0",
    "runme": "^3.0.2",
    "thor-devkit": "^2.0.9",
    "ts-node": "^10.9.1",
    "ts-node-test": "^0.4.3"
  }
}
